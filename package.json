    "webpack-merge": "^5.8.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "ahooks": "^3.7.5",
    "antd": "^5.3.1",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "color-hash": "^2.0.2",
    "compare-versions": "^5.0.3",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.1",
    "electron-debug": "^3.2.0",
