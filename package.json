    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^13.0.0",
    "eslint-config-erb": "^3.0.0",
