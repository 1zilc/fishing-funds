{
  "name": "fishing-funds",
  "productName": "Fishing Funds",
  "description": "基金,大盘,股票,虚拟货币状态栏显示小应用,基于Electron开发,支持 MacOS,Windows,Linux客户端,数据源来自天天基金,蚂蚁基金,爱基金,腾讯证券,新浪基金等",
  "version": "8.0.0",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"--loader tsx\" electron-vite dev",
    "build": "cross-env NODE_ENV=development NODE_OPTIONS=\"--loader tsx\" electron-vite build",
    "build:icon": "electron-icon-builder --input=./assets/icon.png --output=./icon-build",
    "package-all": "node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -mwl -p never",
    "release-all": "node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -mwl -p always",
    "beta-mac": "node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -m -p always"
  },
  "build": {
    "productName": "Fishing Funds",
    "appId": "com.electron.1zilc.fishing-funds",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "copyright": "Copyright © 2023 1zilc",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "fileAssociations": [
      {
        "ext": "ff",
        "role": "Editor",
        "description": "Fishing Funds 备份文件",
        "icon": "assets/icon"
      }
    ],
    "mac": {
      "extendInfo": {
        "LSUIElement": "true"
      },
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ],
      "category": "Financial"
    },
    "snap": {
      "confinement": "strict",
      "summary": "Display real-time trends of Chinese funds in the menubar.",
      "publish": {
        "provider": "snapStore",
        "channels": [
          "stable"
        ]
      }
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "1zilc",
        "repo": "fishing-funds"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1zilc/fishing-funds.git"
  },
  "author": {
    "name": "1zilc",
    "email": "dywzzjx@163.com",
    "url": "https://www.1zilc.top"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/1zilc/fishing-funds/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "macos",
    "linux",
    "homebrew",
    "menubar",
    "typescript",
    "bitcoin",
    "stock",
    "cryptocurrency",
    "alipay",
    "wechat",
    "chinese",
    "antd",
    "echarts",
    "funds",
    "snapcraft"
  ],
  "homepage": "https://github.com/1zilc",
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@electron/rebuild": "^3.2.13",
    "@types/color-hash": "^1.0.2",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0",
    "electron-builder": "^24.4.0",
    "electron-extension-installer": "^1.2.0",
    "electron-vite": "^1.0.23",
    "generate-react-cli": "^8.3.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "source-map-support": "^0.5.21",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite-plugin-mkcert": "^1.15.0"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "ahooks": "^3.7.7",
    "antd": "^5.5.2",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^1.2.1",
    "color-hash": "^2.0.2",
    "compare-versions": "^5.0.3",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "electron-debug": "^3.2.0",
    "electron-disable-file-drop": "^1.0.2",
    "electron-icon-builder": "^2.0.1",
    "electron-log": "5.0.0-beta.24",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.0",
    "electron-window-state": "^5.0.3",
    "got": "^13.0.0",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.0",
    "hxc3-indicator-formula": "^1.0.9",
    "iconv-lite": "^0.6.3",
    "js-base64": "^3.7.5",
    "menubar": "^9.3.0",
    "number-precision": "^1.6.0",
    "promise-worker": "^2.0.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-sortablejs": "^6.1.4",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "socks-proxy-agent": "^8.0.1",
    "sortablejs": "^1.15.0"
  },
  "devEngines": {
    "node": ">=18.x",
    "npm": ">=8.x"
  }
}
