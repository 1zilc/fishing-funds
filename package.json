    "electron-icon-builder": "^2.0.1",
    "electron-log": "5.0.0-beta.23",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.0.0",
    "electron-window-state": "^5.0.3",
    "got": "^12.6.0",
    "html2canvas": "^1.4.1",
    "http-proxy-agent": "^6.0.0",
    "https-proxy-agent": "^5.0.1",
    "hxc3-indicator-formula": "^1.0.9",
    "iconv-lite": "^0.6.3",
    "js-base64": "^3.7.5",
    "json2csv": "^5.0.7",
    "menubar": "^9.3.0",
    "number-precision": "^1.6.0",
    "promise-worker": "^2.0.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-sortablejs": "^6.1.4",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "socks-proxy-agent": "^8.0.0",
    "sortablejs": "^1.15.0"
  },
  "browserslist": [],
  "devEngines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
